{
  "name": "api",
  "version": "1.10.0",
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "yarn dotenv && yarn types && yarn tsoa && yarn dynamodb && yarn openapi && yarn build",
    "build": "tsc",
    "start": "serverless offline start",
    "deploy": "serverless deploy",
    "serverless": "SLS_DEBUG=* serverless",
    "dotenv": "dotenv -e .scaffoldly/.env -c $NODE_ENV -- ngx-scripts env service_urls env_vars --parse-json --out src/env.ts",
    "openapi": "openapi-generator -g axios -i .scaffoldly/$NODE_ENV -o src/services/openapi",
    "types": "ts-node types.ts",
    "tsoa": "node tsoa.js",
    "dynamodb": "serverless dynamodb install",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "module": "",
  "devDependencies": {
    "@ngx-rocket/scripts": "^5.1.0",
    "@scaffoldly/openapi-generator": "^1.0.11",
    "@types/aws-lambda": "^8.10.72",
    "@types/body-parser": "^1.19.0",
    "@types/cookies": "^0.7.6",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/moment": "^2.13.0",
    "@types/node": "12",
    "@types/notp": "^2.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-eslint": "^10.1.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.3.1",
    "serverless": "^2.2.0",
    "serverless-bundle": "^4.3.1",
    "serverless-configuration": "^1.0.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.8.0",
    "serverless-offline-dotenv": "^0.1.2",
    "serverless-plugin-resource-tagging": "^1.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@scaffoldly/serverless-util": "^3.0.32",
    "@vendia/serverless-express": "^4.3.9",
    "axios": "^0.21.1",
    "cookies": "^0.8.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-auth-library": "^6.1.3",
    "joi": "^17.4.0",
    "joi-to-typescript": "^1.14.1",
    "jose": "2",
    "lodash": "^4.17.21",
    "moment": "^2.26.0",
    "node-2fa": "^2.0.2",
    "serverless-http": "^2.7.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.8.0",
    "ulid": "^2.3.0",
    "uuid": "^8.3.2"
  }
